<div class="col-xs-12 col-sm-6 col-md-4">
	<div class="card">
		<a class="img-card" href="">
			<img src="https://images.unsplash.com/photo-1520549233664-03f65c1d1327?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=291a44a7326606a722ab3eddf4e18cf9&auto=format&fit=crop&w=634&q=80" />
		</a>
		<div class="card-content">
			<h4 class="card-title">
				<a href="#">
					{{this.name}}
				</a>
			</h4>
			<div>
				<p>
					Url: {{this.url}}
				</p>
				<p>
					Img: {{this.img}}
				</p>
			</div>
		</div>
	</div>
</div>